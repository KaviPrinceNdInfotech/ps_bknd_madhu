@model HospitalPortal.Models.ViewModels.DoctorslotDTO
@{
    ViewBag.Title = "ViewDoctorSlot";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    int i = 1;
}

<h2>ViewDoctorSlot</h2>
<div id="mainContainer">
    <div class="row">
        <div class="col-md-6">
            @Html.ActionLink("Add Slot Time", "DoctorSlot", null, new { @class = "btn btn-info" })
        </div>
    </div>
    <br />
    <table class="table table-hover">
        <tr>
            <th>
                SN.
            </th>
           
            <th>
                SlotTime
            </th>
            <th>Action</th>
        </tr>

        @foreach (var item in Model.Slotlist)
        {
            <tr>
                <td>
                    @i
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SlotTime)
                </td>
                 
                <td>
                    @*<a href="/AdminOperation/DeleteHealthPackage?Id=@item.Id"  class="btn btn-danger" onclick="return confirm('Are you sure？')"><i class="fa fa-trash-o"></i></a>*@
                    @Html.ActionLink("Delete", "DeleteDoctorSlot", new { id = item.Id }, new { onclick = "return confirm('Are sure wants to delete?');", @class = "btn btn-danger" })
                </td>
            </tr>
            i++;
        }

    </table>
</div>

