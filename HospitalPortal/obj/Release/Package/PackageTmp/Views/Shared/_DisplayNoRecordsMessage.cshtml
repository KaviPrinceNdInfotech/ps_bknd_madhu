
@if (TempData["msg"] != null)
{
    <div class="row">
        <div class="col-md-12">
            @if (TempData["msg"].ToString() == "No")
            {
                <div class="alert alert-success">
                    No Records Available
                </div>
            }
            else
            {
                <div class="alert alert-danger">
                    @TempData["msg"]
                </div>
            }
        </div>
    </div>
}
