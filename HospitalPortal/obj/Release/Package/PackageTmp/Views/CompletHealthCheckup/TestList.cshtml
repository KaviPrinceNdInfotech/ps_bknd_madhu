@model HospitalPortal.Models.ViewModels.HealthCheckupPatientList
@{
    ViewBag.Title = "TestList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2 style="margin-bottom:5%;">Test List of Patient @Model.testlistbyhealthcheckup.FirstOrDefault().PatientName</h2>

<table class="table table-bordered" >
    <thead>
        <tr>
            <th>Patient Name</th>
            <td>@Model.testlistbyhealthcheckup.FirstOrDefault().PatientName</td>
            <th>Patient Address</th>
            <td>@Model.testlistbyhealthcheckup.FirstOrDefault().Location,
            @Model.testlistbyhealthcheckup.FirstOrDefault().CityName,
                @Model.testlistbyhealthcheckup.FirstOrDefault().StateName
            </td>
            <th>Email ID</th>
            <td>
                @Model.testlistbyhealthcheckup.FirstOrDefault().EmailId
            </td>
        </tr>
        <tr>
            <th>Mobile No.</th>
            <td>@Model.testlistbyhealthcheckup.FirstOrDefault().MobileNumber</td>
    </tr>
    </thead>
    <tbody>
        <tr><th>Test List</th></tr>
        @foreach (var item in Model.testlistbyhealthcheckup)
        {
        <tr>
            <td>@item.TestName</td>
            <td><a href="~/HealthCheckUpReports/@item.File" target="_blank" class="btn btn-default">View Test Report</a></td>
        </tr>
        }
    </tbody>
</table>


