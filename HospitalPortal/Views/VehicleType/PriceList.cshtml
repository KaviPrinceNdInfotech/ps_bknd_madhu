@model HospitalPortal.Models.DomainModels.VehicleType
@{
    ViewBag.Title = "PriceList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Price List</h2>
<style>
    #main{
         
        margin:auto;
    }
</style>
<a href="/VehicleType/All" class="btn btn-info">Back</a>
<div id="main">
  
    <table class="table table-bordered">
        <tr>
            <td>Vehicle Name</td>
            <td>₹ @Model.VehicleTypeName</td>
        </tr>
        <tr>
            <td>Driver Charge</td>
            <td>@Model.DriverCharge</td>
        </tr>
        <tr>
            <td>0-5 km</td>
            <td> ₹ @Model.under5KM</td>
        </tr>

        <tr>
            <td>6-10 km</td>
            <td>₹ @Model.under6_10KM</td>
        </tr>
        <tr>
            <td>11-20 km</td>
            <td>₹ @Model.under11_20KM</td>
        </tr>
        <tr>
            <td>21-40 km</td>
            <td>₹ @Model.under21_40KM</td>
        </tr>
        <tr>
            <td>41-60 km</td>
            <td>₹ @Model.under41_60KM</td>
        </tr>

        <tr>
            <td>61-80 km</td>
            <td>₹ @Model.under61_80KM</td>
        </tr>

        <tr>
            <td>81-100 km</td>
