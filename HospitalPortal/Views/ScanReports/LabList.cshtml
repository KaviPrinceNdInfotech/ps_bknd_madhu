@model HospitalPortal.Models.ViewModels.ScanReportVM

<div style="margin:auto">

@using (Html.BeginForm())
{
    <div class="row">
        <div class="col-md-3">
            @Html.TextBoxFor(a=> a.LabName, new{ @class="form-control" })
        </div>
        <div class="col-md-3">
            <input type="submit" value="filter" />
        </div>
    </div>
}


@if (Model.LabList == null)
{
    if (TempData["msg"] != null)
    {
        <h2>@TempData["msg"]</h2>
    }
}
else
{
    <table class="table table-bordered">
        <tr><th>Lab Name</th>
        <th>Action</th>
        </tr>
        <tbody>
            @foreach (var item in Model.LabList)
            {
                <tr>
                    <td>@item.LabName</td>
                    <td><a href="/Lab/ViewUploadReports?id=@item.Id">View Reports</a></td>
                </tr>
            }

        </tbody>
    </table>
}
</div>
